pipeline{
    agent any;
    stages{
        stage('BUILD'){
            steps{
                echo "hi team, building..."
                sh 'sleep 5'
            }
        }
        stage('TEST'){
            steps{
                echo "hi team, testing..."
                sh 'sleep 5'
            }
        }stage('DEPLOY'){
            steps{
                echo "hi team, deploying"
                sh 'sleep 5'
            }
        }
    }
}
